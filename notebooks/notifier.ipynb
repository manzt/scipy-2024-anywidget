    "import time\n",
    "\n",
    "w = Notifier()\n",
    "\n",
    "for i in range(3, 0, -1):\n",
    "    time.sleep(0.5)\n",
    "    print(i)\n",
    "print(\"done\")\n",
    "\n",
    "w.notify(\"Experiment done!\", body=\"The simulation has finished in Jupyter\", image=\"https://cataas.com/cat\")"
